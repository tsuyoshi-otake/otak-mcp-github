{
  "mcpServers": {
    "otak-mcp-commander": {
      "type": "stdio",
      "command": "dotnet",
      "args": [
        "run",
        "--project",
        "./otak-mcp-commander/otak-mcp-commander.csproj",
        "--no-build"
      ],
      "env": {},
      "alwaysAllow": [
        "ListFiles",
        "TailLog",
        "WriteLog",
        "GetCurrentDirectory",
        "GetLogPath",
        "ExecuteCommand"
      ],
      "disabled": false
    }
  }
}